  "scripts": {
    "build:aot:prod": "npm run clean:dist && npm run clean:aot && webpack --config config/webpack.prod.ts  --progress --profile --bail",
    "build:aot": "npm run build:aot:prod",
    "build:dev": "webpack --config config/webpack.dev.ts --progress --profile --colors",
    "build:main": "cross-env NODE_ENV=development webpack --config ./config/webpack.electron.ts --progress --profile --colors",
    "build": "npm-run-all -pr build:main build:dev",
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- 'dist/!(main.js|main.js.map)'",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "postversion": "git push && git push --tags",
    "preclean:install": "npm run clean",
    "preversion": "npm test",
    "rimraf": "del",
    "server:browser:hmr": "npm run server:browser -- --inline --hot",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:browser": "cross-env START_BROWSER=1 webpack-dev-server --config config/webpack.dev.ts --progress --profile --colors",
    "server:dev": "cross-env START_HOT=1 webpack-dev-server --config config/webpack.dev.ts --progress --profile --colors",
    "server:prod": "electron dist/main.js",
    "server": "npm run server:dev",
    "start:hmr": "npm-run-all -pr server:dev:hmr watch:main",
    "start": "npm-run-all -pr server:dev watch:main",
    "test": "npm run lint && karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:test:debug": "npm run watch:test -- --browsers CustomElectron",
    "watch:main": "cross-env START_HOT=1 npm run build:main -- --watch",
    "watch:main:prod": "npm run build:main -- --watch",
    "watch:renderer": "cross-env START_HOT=1 npm run build:dev -- --watch",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron dist/main.js"
  },
